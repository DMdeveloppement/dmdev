---
import type { CollectionEntry } from 'astro:content';
import Interest from './interest.astro';

interface Props {
  interestEntries: CollectionEntry<'interests'>[];
}

const { interestEntries } = Astro.props;
---

<div class="flex flex-wrap gap-x-4 gap-y-3">
  {interestEntries.map((interestEntry) => <Interest interestEntry={interestEntry} />)}
</div>
