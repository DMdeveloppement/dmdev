---
import type { CollectionEntry } from 'astro:content';

import Interest from './interest.astro';

interface Props {
  entries: CollectionEntry<'interests'>[];
}

const { entries } = Astro.props;
---

<div class="flex flex-wrap gap-x-4 gap-y-3">
  {entries.map((interestEntry) => <Interest {interestEntry} />)}
</div>
